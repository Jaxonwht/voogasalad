        <myID>100</myID>
        <myExecution>$clicked.props.hp = $clicked.props.hp - selected.props.dmg
            if($clicked.props.hp &lt;= 0) { GameData.removeEntity($clicked) }
            GameData.toNextPlayer()
            GameData.goTo(&apos;A&apos;.hashCode())</myExecution>
    </gameplay.Node>
