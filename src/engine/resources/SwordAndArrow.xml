        <myGuard>if(GameData.isTile($clicked) &amp;&amp; GameData.distance($clicked, selected) &lt;= 1 &amp;&amp; GameData.hasNoEntities($clicked)) { $return = true } else { $return = false }</myGuard>
    </gameplay.Edge>
